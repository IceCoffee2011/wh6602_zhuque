[set]
;过滤的文件名及扩展名
filter=UNWISE.EXE;updateset.ini;updateset.dat;update.ini;BRAnyChatCore.log;msvcr71d.dll;msvcr71.dll;msvcp71d.dll;msvcp71.dll;mfc71d.dll;MFC71.dll;*.log;*.db;*.map;*.ilk;*.scc;*.txt;*.pdb;*.map;*.ilk;*.lib;*.exp;
;下载地址
URL=http://dls.cbwgame.com/update/hallfile/

;记录安装过程的日志
WriteLog=1
;下载线程数目
ThreadCount=1
;下载模式
DownloadMode=0

;强制更新目录与文件
[enforce]
directory1=\data\
directory2=\config\
directory3=\Face\
directory4=\Url\
directory5=\LandCrazy\
directory6=\SparrowYY\
directory7=\SparrowZZ\
directory8=\cbwGameUpdate.exe
directory9=\GamePlaza.exe
directory10=\LandCrazy.exe
directory11=\SparrowYY.exe
directory12=\SparrowZZ.exe
directory13=\cbwUpdateHelp.exe
directory14=


;目录为单位组成更新包
[directory]
unit1=\data\ClientShare\
unit2=\data\gameframe\
unit3=\data\guide\
unit4=\data\image\
unit5=\data\logon\
unit6=\data\music\
unit7=\data\plaza\
unit8=\data\room\
unit9=\Data\status\
unit10=\Face\
unit11=\LandCrazy\
unit12=\SparrowYY\
unit13=\SparrowZZ\
unit14=\config\
unit15=\Url\
unit16=

;辅助更新文件（即：优先更新）
[priority]
file1=cbwGameUpdate.exe
file2=SimpleTask.dll
file3=SegmentDownload.dll
file4=cbwUpdateHelp.exe
file5=UM.dll